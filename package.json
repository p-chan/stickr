{
  "name": "stickr",
  "version": "1.0.0",
  "description": "A Slackbot to use LINE Stickers in Slack",
  "license": "UNLICENSED",
  "author": "P-Chan",
  "main": "./dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/p-chan/stickr.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dev": "ts-node ./src",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "start": "node .",
    "watch": "nodemon --watch \"./src/**/*\" --exec \"ts-node\" ./src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^2.7.1",
    "@slack/bolt": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@p-chan/prettier-config": "^2.0.0",
    "@p-chan/tsconfig": "^1.1.0",
    "@prisma/cli": "^2.7.1",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.11.2",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "private": true
}
