{
  "name": "stickr",
  "version": "1.0.0",
  "description": "A Slackbot to use LINE Stickers in Slack",
  "license": "UNLICENSED",
  "author": "P-Chan",
  "main": "./dist",
  "repository": {
    "type": "git",
    "url": "https://github.com/p-chan/stickr.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "db:client:generate": "prisma generate",
    "db:migrate:save": "prisma migrate save --experimental",
    "db:migrate:up": "prisma migrate up --experimental",
    "dev": "ts-node ./src",
    "format": "prettier --write .",
    "postinstall": "npm run db:client:generate",
    "prebuild": "npm run clean",
    "start": "node .",
    "watch": "nodemon --watch \"./src/**/*\" --exec \"ts-node\" ./src/index.ts"
  },
  "dependencies": {
    "@prisma/cli": "^2.8.0",
    "@prisma/client": "^2.7.1",
    "@slack/bolt": "^2.3.0",
    "axios": "^0.20.0",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@p-chan/prettier-config": "^2.0.0",
    "@p-chan/tsconfig": "^1.1.0",
    "@types/cheerio": "^0.22.22",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "husky": "^4.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "private": true
}
