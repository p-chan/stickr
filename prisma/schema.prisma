datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Installation {
  id        Int      @default(autoincrement()) @id
  teamId    String   @unique
  raw       Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
